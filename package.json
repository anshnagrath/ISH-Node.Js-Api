{
  "name": "typescript-mongoose",
  "version": "1.0.0",
  "license": "MIT",
  "author": "ansh",
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "scripts": {
    "build": "tsc -p ./prod.tsconfig.json && ncp './server/assets' './dist/assets'",
    "lint": "tslint server/**/*.ts --fix",
    "start": "nodemon"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "server/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "exclude": [
      "server/**/*.d.ts",
      "server/tests/**/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary"
    ],
    "check-coverage": "--lines 50 --per-file",
    "all": false,
    "sourceMap": false,
    "instrument": true
  },
  "dependencies": {
    "aws-sdk": "^2.223.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-mung": "^0.5.0",
    "express-pdf": "^1.2.2",
    "express-sharp": "^2.1.1",
    "file-system": "^2.2.2",
    "mockgoose-fix": "~7.3.6",
    "mongoose": "^4.13.6",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "request": "^2.85.0",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^3.0.6",
    "urlencode": "^1.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.4",
    "@types/express": "^4.0.36",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.7",
    "babel-cli": "^6.26.0",
    "dotenv": "^4.0.0",
    "isparta": "^4.0.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.12.1",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0",
    "tslint": "^5.4.3",
    "typescript": "^2.9.1"
  }
}
